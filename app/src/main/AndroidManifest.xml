<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.myqueue.myqueue" >

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Activities.SplashActivity"
            android:theme="@style/Theme.MyThemeTrans">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Activities.LoginActivity"   android:theme="@style/Theme.MyThemeTrans"></activity>
        <activity android:name=".Activities.SignupActivity"   android:theme="@style/Theme.MyThemeTrans"></activity>
        <activity android:name=".Activities.HomeActivity"  android:theme="@style/AppTheme"></activity>

        <activity
            android:name=".Activities.NewsFeedFormActivity"
            android:label="" >
        </activity>
        <activity
            android:name=".Activities.BookScreenActivity"
            android:label="">
        </activity>
        <activity
            android:name=".Activities.WaitingListActivity"
            android:label="">
        </activity>

    </application>

</manifest>
